<window
        xmlns="http://schemas.haulmont.com/cuba/4.0/window.xsd"
        class="com.haulmont.cuba.gui.report.browse.InputParametersController"
        messagesPack="com.haulmont.cuba.gui.report"
        caption="msg://inputParameters"
        >

    <dsContext>
    </dsContext>

    <layout expand="parametersGrid">
        <vbox spacing="true">
            <grid id="parametersGrid" spacing="true">
                <columns>
                    <column flex="1"/>
                    <column flex="2"/>
                </columns>
                <rows>
                    <row>
                    </row>
                </rows>
            </grid>
            <button id="printReportBtn" invoke="printReport"
                    caption="msg://input.printReport" icon="icons/print.png"/>
        </vbox>

    </layout>
</window>
